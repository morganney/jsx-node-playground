{
  "name": "jsx-node-playground",
  "version": "1.0.0",
  "description": "Node.js SSR + Hydration using React without TSX or JSX",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "prettier": "prettier -w .",
    "check:ssr:ts": "tsx scripts/check-ssr-ts.ts",
    "bundle": "rspack --config rspack.config.js",
    "generate:types": "knighted-css-generate-types --root . --include src",
    "postinstall": "npm run generate:types",
    "start": "node dist/server.js",
    "check-types": "tsc --noEmit",
    "precheck-types": "npm run generate:types"
  },
  "dependencies": {
    "@knighted/css": "^1.1.1",
    "@knighted/jsx": "^1.7.5",
    "@rspack/cli": "^1.7.0",
    "@rspack/core": "^1.7.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "linkedom": "^0.18.12",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 90,
    "semi": false,
    "singleQuote": true
  }
}
